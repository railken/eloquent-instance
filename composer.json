{
    "name": "railken/eloquent-instance",
    "type": "library",
    "keywords": ["laravel", "eloquent-instance"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/database": "9.* || 10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "nilportugues/sql-query-formatter": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\EloquentInstance\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\EloquentInstance\\Tests\\": "tests/" 
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
